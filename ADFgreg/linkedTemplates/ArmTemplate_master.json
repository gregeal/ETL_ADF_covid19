{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFgreg"
		},
		"AzureSqlDataset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDataset'"
		},
		"ls_ablob_gregblobsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_gregblobsa'"
		},
		"ls_adls_adlsgreg2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_adlsgreg2'"
		},
		"ls_adls_raw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_raw'"
		},
		"ADLSLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgreg.dfs.core.windows.net/"
		},
		"RestAPILinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().restURL}"
		},
		"ls_adls_adlsgreg2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgreg2.dfs.core.windows.net/"
		},
		"ls_adls_cases_deaths_csv1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_adls_raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgreg2.dfs.core.windows.net/"
		},
		"ls_adlsgen1_raw_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgreg.dfs.core.windows.net"
		},
		"tr_ingest_from_sorce_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/449912a3-fc18-46b6-bf5c-d07a0748901e/resourceGroups/gregRG/providers/Microsoft.Storage/storageAccounts/gregblobsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFgreg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDataset_connectionString": {
						"value": "[parameters('AzureSqlDataset_connectionString')]"
					},
					"ls_ablob_gregblobsa_connectionString": {
						"value": "[parameters('ls_ablob_gregblobsa_connectionString')]"
					},
					"ls_adls_adlsgreg2_accountKey": {
						"value": "[parameters('ls_adls_adlsgreg2_accountKey')]"
					},
					"ls_adls_raw_accountKey": {
						"value": "[parameters('ls_adls_raw_accountKey')]"
					},
					"ADLSLinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADLSLinkedService_properties_typeProperties_url')]"
					},
					"RestAPILinkedService_properties_typeProperties_url": {
						"value": "[parameters('RestAPILinkedService_properties_typeProperties_url')]"
					},
					"ls_adls_adlsgreg2_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_adlsgreg2_properties_typeProperties_url')]"
					},
					"ls_adls_cases_deaths_csv1_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_cases_deaths_csv1_properties_typeProperties_url')]"
					},
					"ls_adls_raw_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_raw_properties_typeProperties_url')]"
					},
					"ls_adlsgen1_raw_csv_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen1_raw_csv_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFgreg_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_from_sorce_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_from_sorce_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFgreg_ArmTemplate_0')]"
			]
		}
	]
}